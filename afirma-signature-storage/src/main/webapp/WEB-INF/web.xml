<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
  <display-name>SignatureStorageServer</display-name>
  <servlet>
    <description>Servicio de almacenamiento temporal de firmas v1.4</description>
    <servlet-name>StorageService</servlet-name>
    <servlet-class>es.gob.afirma.signfolder.server.proxy.StorageService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StorageService</servlet-name>
    <url-pattern>/StorageService</url-pattern>
  </servlet-mapping>
</web-app>